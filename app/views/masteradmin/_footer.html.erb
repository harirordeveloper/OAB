<script type="text/javascript">
        $(function () {
            $(document).keyup(function (e) {
                var key = (e.keyCode ? e.keyCode : e.charCode);
                switch (key) {
                   case 113:
                        navigateUrl('/home/company');
                        break;
						 case 115:
                        navigateUrl('/home/customer');
                        break;
						 case 116:
                        navigateUrl('/home/products');
                        break;
						 case 118:
                        navigateUrl('/home/invoices');
                        break;
                    case 119:
                        navigateUrl('/home/sales');
                        break;
                    case 120:
                        navigateUrl('/home/payments');
                        break;
                    case 121:
                        navigateUrl('/home/reports');
                        break;
                    default: ;
                }
            });
            function navigateUrl(jObj) {
                window.top.location = jObj;
            }
        });       
    </script>




