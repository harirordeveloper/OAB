<script name="text/javascript">
    function calls()
       {
         clinic = <%= @labid %>
            
         selectedtest =  $( "#test_master_test_name" ).val();
     
        //$( '#selected_test' ).append(selectedtest + '\n');
       $('#test_master_test_name').val("")
        

        jQuery.ajax({
                  url: '/lab_flows/branch',
		     	type: "POST",
                        data: { branch: selectedtest, clinic: clinic },
                        beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
                       
               });
	
	
    }
      
</script>

<script type="text/javascript">
    function state()
       {
         country = $('#country_id').val();
           jQuery.ajax({
                  url: '/home/state',
		     	type: "POST",
                        data: { country_val: country },
                        beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
                        success: function(result){
                            
                                $( '#state_id' ).empty();
                                $('#state_id').append('<option value="'+ '' + '">' + "Select State" + '</option>');
                                    $.each(result,function(key,val) {
                                           
                                           $('#state_id').append('<option value="'+ val.id + '">' + val.name + '</option>');
                                                  });
                                                               },  
                                     error: function(){
                                          alert('No States Found');
                                          }
               });
	
    }
   
</script>
<script type="text/javascript">
    function city()
       {

         state = $('#state_id').val();
           jQuery.ajax({
                  url: '/home/city',
		     	type: "POST",
                        data: { country_val: state },
                        beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},

                        success: function(result){
                            
                                $( '#city_id' ).empty();
                                $('#city_id').append('<option value="'+ '' + '">' + "Select City" + '</option>');
                                    $.each(result,function(key,val) {
                               
                                           $('#city_id').append('<option value="'+ val.id + '">' + val.name + '</option>');
                                                  });
                                                               },  
                                     error: function(){
                                          alert('No Cities Found');
                                          }
               });
	
    }
   
</script>


<script type="text/javascript">
    function clinicsearch()
       {
         
         country = $('#country_id').val();
         state = $('#state_id').val();
         city = $('#city_id').val();
         clinic=$('#address1').val();
           jQuery.ajax({
                  url: '/home/searchclinic',
		     	type: "POST",
                        data: {clinic_val: clinic,country_val: country,state_val: state,city_val: city },
                        beforeSend: function(xhr) {xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))},
                        success: function(result){
                                      
                                            jQuery('#set_id').html(result);
                               
                                             
                                                               },  
                                     error: function(){
                                          alert('No States Found');
                                          }
               });
	
    }
   
</script>


